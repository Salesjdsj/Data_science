      },
      "source": [
        "# Gráfico de barras para os 5 países com mais casos\n",
        "fig, ax = plt.subplots(figsize=(10,6), dpi=72)\n",
        "\n",
        "# Definindo os dados a serem plotados\n",
        "top5_total_cases = df_top5cases.loc[df_top5cases.date == '2021-08-14', (['location', 'total_cases'])].sort_values(by=\"total_cases\", \n",
        "                                                                                              ascending=False)[0:5]\n",
        "                                                                          \n",
        "# Definindo o formato do gráfico\n",
        "ax = sns.barplot(x=\"location\", y=\"total_cases\", data=top5_total_cases,\n",
        "                 palette=\"Blues_r\")\n",
        "\n",
        "# Retirando eixos do gráfico\n",
        "ax.spines['top'].set_visible(False)\n",
        "ax.spines['right'].set_visible(False)\n",
        "\n",
        "# Inserindo o título e subtítulo\n",
        "plt.text(-0.033, 1.2,  \"Total de casos \", transform=ax.transAxes, fontsize=18, fontweight='bold', color=\"#58555A\")\n",
        "plt.text(-0.033, 1.15,  \"Os 5 países com o maior número de casos atribuídos ao COVID-19 até 2021-08-14\", \n",
        "         transform=ax.transAxes, fontsize=12, fontweight='bold', color=\"#A1A5A6\")\n",
        "\n",
        "plt.tight_layout()\n",
        "\n",
        "# Salvar Imagem\n",
        "fig.savefig('barrastotaldecasos.png')"
