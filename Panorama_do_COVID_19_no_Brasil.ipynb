      "source": [
        "# Gráfico de barras para os 5 países com mais doses administradas\n",
        "fig, ax = plt.subplots(figsize=(10,6), dpi=72)\n",
        "\n",
        "# Definindo os dados a serem plotados\n",
        "top5sam = df_vacsam.loc[df_vacsam.date == '2021-08-14', ['location', 'total_vaccinations']].sort_values(by=\"total_vaccinations\", \n",
        "                                                                                              ascending=False)[0:5]\n",
        "\n",
        "# Definindo o formato do gráfico\n",
        "ax = sns.barplot(x=\"location\", y=\"total_vaccinations\", data=top5sam,\n",
        "                 palette=\"Blues_r\")\n",
        "\n",
        "# Retirando os eixos\n",
        "ax.spines['top'].set_visible(False)\n",
        "ax.spines['right'].set_visible(False)\n",
        "\n",
        "# Inserindo o título e subtítulo\n",
        "plt.text(-0.08, 1.2,  \"Total de vacinas administradas\", transform=ax.transAxes, fontsize=18, fontweight='bold', color=\"#58555A\")\n",
        "plt.text(-0.08, 1.15,  \"Os 5 países da América do Sul com o maior número de doses de vacinas administradas até 2021-08-14\", \n",
        "         transform=ax.transAxes, fontsize=12, fontweight='bold', color=\"#A1A5A6\")\n",
        "\n",
        "plt.tight_layout()\n",
        "\n",
        "# Salvar Imagem\n",
        "fig.savefig('totaldevacinasamerica.png')"
