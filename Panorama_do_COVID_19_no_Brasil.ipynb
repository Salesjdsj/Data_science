      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "dJbXlqpCUUG5",
        "outputId": "ea812d00-6688-4a7d-d7ef-0b0acf58fbdd"
      },
      "source": [
        "# Identificar a porcentagem de valores ausentes das colunas\n",
        "(df.isnull().sum() / df.shape[0]).sort_values(ascending=False)"
      ],
